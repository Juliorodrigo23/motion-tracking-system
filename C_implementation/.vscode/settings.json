{
    "C_Cpp.default.includePath": [
        "${workspaceFolder}/**",
        "/opt/homebrew/include/**",
        "/opt/homebrew/Cellar/opencv/4.10.0_18/include/**",
        "/opt/homebrew/Cellar/eigen/3.4.0_1/include/**",
        "/opt/homebrew/Cellar/protobuf/29.2/include/**",
        "${workspaceFolder}/mediapipe",
        "${workspaceFolder}/mediapipe/bazel-mediapipe/external/com_google_protobuf/src",
        "${workspaceFolder}/mediapipe/bazel-bin"
    ],
    "C_Cpp.default.compilerPath": "/usr/bin/clang++",
    "C_Cpp.default.cStandard": "c17",
    "C_Cpp.default.cppStandard": "c++17",
    "C_Cpp.default.intelliSenseMode": "macos-clang-arm64",
    "files.associations": {
        "*.hpp": "cpp",
        "*.cpp": "cpp"
    },
    "C_Cpp.default.defines": [
        "MEDIAPIPE_DISABLE_GPU=1"
    ]
}